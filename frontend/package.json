{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "description": "DDL-Checker",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "set NODE_ENV=production && taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "set NODE_ENV=production && npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@nutui/nutui-taro": "^3.1.20",
    "@tarojs/components": "^3.4.10",
    "@tarojs/plugin-html": "^3.4.10",
    "@tarojs/plugin-framework-vue3": "^3.4.10",
    "@tarojs/plugin-platform-weapp": "^3.4.10",
    "@tarojs/runtime": "^3.4.10",
    "@tarojs/taro": "^3.4.10",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@tarojs/mini-runner": "^3.4.10",
    "@tarojs/webpack-runner": "^3.4.10",
    "@types/webpack-env": "^1.17.0",
    "@vue/compiler-sfc": "^3.2.36",
    "babel-preset-taro": "^3.4.10",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.16.0",
    "eslint-config-taro": "^3.4.10",
    "eslint-plugin-vue": "^9.0.0",
    "stylelint": "^14.8.5",
    "vue-loader": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "typescript": "^4.7.2"
  }
}
